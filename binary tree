#include<iostream>
using namespace std;
#include<string.h>
struct node
{
      string data;
      struct node*right;
      struct node*left;

      node(string val)
      {
            data=val;
            left=NULL;
            right=NULL;
      }
};

void PreOrder(struct node *temp) {
  if (temp != NULL) {
    cout << "  " << temp->data;
    PreOrder(temp->left);
    PreOrder(temp->right);
  }
}

int main()
{

      struct node*root=new node("FDS");
      root->left=new node("HASH");
      root->right=new node("TREE");
      /*        A
               /  \
               B   C

      */

      root->left->left=new node("HASHING");
      root->left->right=new node("COLLISION");

      root->right->left=new node("GENERIC");
      root->right->right=new node("BINARY");
       /*        A
                / \
               B   C
              / \  / \
              D E  F  G

      */
      root->left->left->left=new node("keys");
      root->left->left->right=new node("table");

      root->right->left->right=new node("PARENT");
      root->right->right->left=new node("search");
      /*
                 A
                / \
               B   C
              / \  / \
              D E  F   G
             /   \  \  /
            H     I  j k
      */
      PreOrder(root);

}
